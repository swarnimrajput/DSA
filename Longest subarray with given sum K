class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        unordered_map<long long,int>mp;
        mp[0]=1;
        long long sum=0;
        int ans=0;

        for(int x:nums){
            sum+=x;
            if(mp.count(sum-k)){
                ans+=mp[sum-k];
            }
            mp[sum]++;
        }
        return ans;}
};
