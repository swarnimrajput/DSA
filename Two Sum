class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int,int>s;
        for(int i=0;i<nums.size();i++){
            int compli=target-nums[i];
            if(s.find(compli)!=s.end()){
                return {s[compli],i};
            }
            s[nums[i]]=i;
        }
        return {};
    }
};
